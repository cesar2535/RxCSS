{
  "name": "rxcss",
  "version": "0.10.0",
  "description": "RxJS and CSS",
  "main": "lib/index.js",
  "scripts": {
    "build:commonjs": "cross-env NODE_ENV=cjs rollup -c config/rollup.config.js -o lib/rxcss.js",
    "build:es": "cross-env BABEL_ENV=es NODE_ENV=es rollup -c config/rollup.config.js -o es/rxcss.js",
    "build:umd": "cross-env BABEL_ENV=es NODE_ENV=development rollup -c config/rollup.config.js -o dist/rxcss.js",
    "build:umd:min": "cross-env BABEL_ENV=es NODE_ENV=production rollup -c config/rollup.config.js -o dist/rxcss.min.js",
    "build": "npm run build:commonjs && npm run build:es && npm run build:umd && npm run build:umd:min",
    "test:watch": "npm test -- --watch",
    "test:cov": "npm test -- --coverage",
    "test": "cross-env BABEL_ENV=commonjs jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/davidkpiano/RxCSS.git"
  },
  "keywords": [
    "RxJS",
    "CSS"
  ],
  "author": "David Khourshid",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/davidkpiano/RxCSS/issues"
  },
  "homepage": "https://github.com/davidkpiano/RxCSS#readme",
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@babel/register": "^7.5.5",
    "chai": "~4.2.0",
    "cross-env": "^5.2.1",
    "eslint": "^6.3.0",
    "jsdom": "~15.1.1",
    "mocha": "~6.2.0",
    "rollup": "^1.20.3",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-replace": "^2.2.0",
    "rollup-plugin-size-snapshot": "^0.10.0",
    "rollup-plugin-typescript2": "^0.24.0",
    "rollup-plugin-uglify": "^6.0.3",
    "rollup-plugin-visualizer": "^2.5.4",
    "ts-jest": "^24.0.2",
    "tslib": "^1.10.0",
    "typescript": "^3.6.2"
  },
  "dependencies": {
    "rxjs": "^6.5.2"
  }
}
